{
  "name": "@kensoni/react-icons",
  "version": "4.2.0",
  "description": "Custom react-icons package",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "source": "src/index.tsx",
  "scripts": {
    "build:esm": "tsc -p ./types/tsconfig.json",
    "build:cjs": "tsc -m commonjs --outDir dist/cjs -p ./types/tsconfig.json",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngvcanh/react-icons-custom.git"
  },
