  "bugs": {
    "url": "https://github.com/ngvcanh/react-icons-custom/issues"
  },
  "homepage": "https://github.com/ngvcanh/react-icons-custom#readme",
  "dependencies": {
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-icons": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
